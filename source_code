CREATE TABLE "m3kashif"."USERS" 
   (	"USERID" NUMBER NOT NULL ENABLE, 
	"USERNAME" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"PASSWORD" VARCHAR2(10 BYTE) NOT NULL ENABLE, 
	"EMAIL" VARCHAR2(250 BYTE) NOT NULL ENABLE, 
	"ROLE" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "USERS_PK" PRIMARY KEY ("USERID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE" ;

COMMENT ON COLUMN "m3kashif"."USERS"."ROLE" IS 'BUYER/ SELLER';

CREATE TABLE "m3kashif"."AUCTION" 
   (	"AUCTIONID" NUMBER NOT NULL ENABLE, 
	"CARID" NUMBER NOT NULL ENABLE, 
	"STARTTIME" TIMESTAMP (6) NOT NULL ENABLE, 
	"ENDTIME" TIMESTAMP (6) NOT NULL ENABLE, 
	"RESERVEPRICE" NUMBER, 
	"CURRENTBID" NUMBER, 
	"DETAILS" NVARCHAR2(20) NOT NULL ENABLE, 
	"STATUS" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"STARTINGBID" NUMBER NOT NULL ENABLE, 
	 CONSTRAINT "AUCTION_PK" PRIMARY KEY ("AUCTIONID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE"  ENABLE, 
	 CONSTRAINT "FK_CARID_AUCTION" FOREIGN KEY ("CARID")
	  REFERENCES "m3kashif"."CARS" ("CARID") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE" ;

CREATE TABLE "m3kashif"."BIDS" 
   (	"BIDID" NUMBER NOT NULL ENABLE, 
	"AUCTIONID" NUMBER NOT NULL ENABLE, 
	"USERID" NUMBER NOT NULL ENABLE, 
	"BIDAMOUNT" NUMBER NOT NULL ENABLE, 
	"BIDTIME" NUMBER NOT NULL ENABLE, 
	 CONSTRAINT "BIDS_PK" PRIMARY KEY ("BIDID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE"  ENABLE, 
	 CONSTRAINT "FK_AUCTIONID_BIDS" FOREIGN KEY ("AUCTIONID")
	  REFERENCES "m3kashif"."AUCTION" ("AUCTIONID") ENABLE, 
	 CONSTRAINT "FK_USERID_BIDS" FOREIGN KEY ("USERID")
	  REFERENCES "m3kashif"."USERS" ("USERID") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE" ;

CREATE TABLE "m3kashif"."CARS" 
   (	"CARID" NUMBER NOT NULL ENABLE, 
	"MAKE" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"MODEL" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"YEAR" NUMBER NOT NULL ENABLE, 
	"MILEAGE" NUMBER NOT NULL ENABLE, 
	"VIN" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"DESCRIPTION" VARCHAR2(250 BYTE) NOT NULL ENABLE, 
	"USERID" NUMBER NOT NULL ENABLE, 
	"IMAGEURL" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "CARS_PK" PRIMARY KEY ("CARID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE"  ENABLE, 
	 CONSTRAINT "FK_USERID_CARS" FOREIGN KEY ("USERID")
	  REFERENCES "m3kashif"."USERS" ("USERID") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DBCOURSE" ;

CREATE TABLE "m3kashif"."TRANSACTIONS" 
   (	"TRANSACTIONID" NUMBER NOT NULL ENABLE, 
	"AUCTIONID" NUMBER NOT NULL ENABLE, 
	"USERID" NUMBER NOT NULL ENABLE, 
